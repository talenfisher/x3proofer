{
    "name": "X3Proofer",
    "version": "0.0.0",
    "description": "Chrome extension for detecting anomalies in X3P files",
    "manifest_version": 2,
    "permissions": [],
    "icons": {
        "128": "favicon.png"
    },
    "browser_action": {
        "default_icon": {
            "128": "favicon.png"
        }
    },
    "options_page": "settings.html",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}

